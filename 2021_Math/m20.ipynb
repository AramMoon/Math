    "import math\n",
    "\n",
    "n = 8\n",
    "\n",
    "def f(x):\n",
    "    return math.pi * math.sin( 2 * math.pi * n * x)\n",
    "\n",
    "i = range(-100, 100)\n",
    "s = 0\n",
    "for k in i:\n",
    "    xx = k * 0.01\n",
    "    yy = f(xx)\n",
    "    s += yy * 0.01\n",
    "print(\"s\", s)\n"
   ]
